{
  "name": "member-sort-extension-for-google-meet",
  "version": "1.1.0",
  "description": "member-sort-extension-for-google-meet",
  "main": "index.js",
  "scripts": {
    "watch": "webpack --config webpack/webpack.dev.js --watch",
    "build": "prettier --write \"src/**/*.{ts,tsx}\" & webpack --config webpack/webpack.prod.js",
    "clean": "rimraf dist",
    "style": "prettier --write \"src/**/*.{ts,tsx}\"",
    "lint": "run-p lint:*",
    "lint:es": "eslint --fix './src/**/*.{ts,tsx}'",
    "lint:ts": "tsc --noEmit",
    "format": "prettier --write 'src/**/*'",
    "check": "npm run lint && npm run format && npm run test",
    "test": "npx jest",
    "commit": "lint-staged -v",
    "prepare": "husky install"
  },
  "author": "",
  "license": "MIT",
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix",
      "bash -c 'npm run lint:ts'",
      "bash -c 'npm run test'"
    ]
  },
  "dependencies": {
    "lodash": "4.17.21",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@types/chrome": "0.0.180",
    "@types/lodash": "4.14.181",
    "@types/react": "17.0.43",
    "@types/react-dom": "17.0.14",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "copy-webpack-plugin": "10.2.4",
    "eslint": "^8.14.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.29.4",
    "husky": "^7.0.4",
    "lint-staged": "^12.4.1",
    "npm-run-all": "^4.1.5",
    "prettier": "2.6.2",
    "rimraf": "3.0.2 ",
    "ts-jest": "27.1.4",
    "ts-loader": "9.2.8",
    "typescript": "4.6.3",
    "webpack": "5.72.0",
    "webpack-cli": "4.9.2",
    "webpack-merge": "5.8.0"
  }
}
