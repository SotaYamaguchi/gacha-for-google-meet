{
  "name": "member-sort-extension-for-google-meet",
  "version": "1.1.0",
  "description": "member-sort-extension-for-google-meet",
  "main": "index.js",
  "scripts": {
    "watch": "webpack --config webpack/webpack.dev.js --watch",
    "build": "npm run style & webpack --config webpack/webpack.prod.js",
    "clean": "rimraf dist",
    "style": "prettier --write \"src/**/*.{ts,tsx}\"",
    "lint": "run-p lint:*",
    "lint:es": "eslint --fix './src/**/*.{ts,tsx}'",
    "lint:ts": "tsc --noEmit",
    "check": "npm run lint && npm run style",
    "test": "npx jest",
    "commit": "lint-staged -v",
    "prepare": "husky install"
  },
  "author": "",
  "license": "MIT",
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix",
      "bash -c 'npm run lint:ts'"
    ]
  },
  "dependencies": {
    "lodash": "4.17.21",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/chrome": "0.0.195",
    "@types/lodash": "4.14.184",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "5.35.1",
    "@typescript-eslint/parser": "5.35.1",
    "copy-webpack-plugin": "11.0.0",
    "eslint": "8.23.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-standard": "17.0.0",
    "eslint-import-resolver-typescript": "3.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-n": "15.2.5",
    "eslint-plugin-promise": "6.0.1",
    "eslint-plugin-react": "7.31.1",
    "eslint-plugin-sort-keys-fix": "1.1.2",
    "eslint-plugin-typescript-sort-keys": "2.1.0",
    "eslint-plugin-unicorn": "47.0.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "rimraf": "3.0.2 ",
    "ts-jest": "28.0.8",
    "ts-loader": "9.3.1",
    "typescript": "4.8.2",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0",
    "webpack-merge": "5.8.0"
  }
